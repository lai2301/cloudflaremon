{
  "groups": [
    {
      "id": "office-computers",
      "name": "Office Computets",
      "services": ["showroom-pc","shop-mac","um760"],
      "uptimeThresholdSet": "strict",
      "stalenessThreshold": 300,
      "auth": {
        "required": true
      },
      "notifications": {
        "enabled": true,
        "channels": ["telegram"],
        "events": ["down", "up", "degraded"]
      }
    }
  ],
  "services": [
    {
      "id": "showroom-pc",
      "name": "Showroom PC",
      "enabled": true
    },
    {
      "id": "shop-mac",
      "name": "Shop Mac",
      "enabled": true
    },
    {
      "id": "um760",
      "name": "UM760",
      "enabled": true
    },
    {
      "id": "unraid-n1",
      "name": "Unraid N1",
      "enabled": true,
      "stalenessThreshold": 300,
      "notifications": {
        "enabled": true,
        "channels": ["telegram"],
        "events": ["down", "up", "degraded"]
      }
    }
  ]
}

